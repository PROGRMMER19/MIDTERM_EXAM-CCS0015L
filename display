void display()
{
    node *p;
    if (front==NULL)
        cout << "Nothing to display\n";
    else{
        cout << "Clients in queue with ticket number: ";
        p = front;
        while (p != NULL) {
            cout << " | " << p->data << " | ";
            p=p->next;
        }
    }
}

